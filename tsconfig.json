{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es5"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true /* Skip type checking all .d.ts files. */
  }
}
